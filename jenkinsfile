                echo 'building'
            }
        }
        stage('Deploy') {
            steps {
                echo 'deploying the app'
            }
        }
        stage('Test') {
            steps {
                echo 'Testing'
            }
        }
        stage('Release') {
            steps {
                echo 'Releasing the app'
            }
        }
    }
}
