pipeline {
    agent any{label 'Jenkins-Agent'}
    tools{
        jdk 'Java17'
        maven 'Maven3'
    }
    stages {
        stage('Build') {
            steps {
                echo 'building'
            }
        }
        stage('Deploy') {
            steps {
                echo 'deploying the app'
            }
        }
        stage('Test') {
            steps {
                echo 'Testing'
            }
        }
        stage('Release') {
            steps {
                echo 'Releasing the app'
            }
        }
        stage("SonarQube Analysis"){
            steps{
	        script{
		     withSonarQubeEnv(credentialsId: 'sonarqube-jenkins-token') { 
                     sh "mvn sonar:sonar"
		     }
	        }	
            }
        }
    }
}
