pipeline {
    agent {label: 'Jenkins Agent'}

    stages {
        stage('Build') {
            steps {
                echo 'Building..'
            }
        }
        stage('Test') {
            steps {
                echo 'Testing..'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying....'
            }
        }
        stage("SonarQube Analysis"){
           steps {
	           script {
		        withSonarQubeEnv(installationName: 'sonarqube-server') { 
                        sh "mvn sonar:sonar"
		          }
	           }	
             }
        }
    }
}
